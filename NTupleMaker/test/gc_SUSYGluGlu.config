[global]
module  = CMSSW_Advanced
; change to your working directory

workdir = /nfs/dust/cms/user/fcost/tools/grid-jobs/work.gc_MiniAODv2_76x_MVAMET0p6/
backend = local

[storage]
se input timeout  = 6:00
se output timeout = 6:00
se output files   = output_MC.root
se output pattern = @NICK@/@NICK@_@MY_JOBID@.root
se path           = 
;  change to the directory where plain ntuples will be stored
         /nfs/dust/cms/user/fcost/store/76x_MiniAODv2_MVAMET0p6/

